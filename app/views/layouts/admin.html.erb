<!DOCTYPE html>
<html>
  <head>
    <title>JobListing</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class='main-container'>
    <div class="container-fluid">
    <%= render "common/navbar"%>
    <%= render "common/flashes"%>
    <div class="row">
      <div class="col-md-2">
        <ul class="nav nav-pills nav-stacked" style="max-width: 200px; padding-top:20px;">
     <li class="btn btn-default"><%= link_to("任务页", jobs_path ) %> </li>
      <li class="btn btn-default"><%= link_to("首页", root_path ) %> </li>

  </ul>
    </div>
    <div class="col-md-10">
    <%= yield %>
  </div>
</div>
<div>
  <%= render "common/footer"%>

</body>
</html>
